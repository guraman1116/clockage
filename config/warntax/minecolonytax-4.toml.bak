[General]
	#Tax generation interval in minutes
	#Range: 1 ~ 1440
	TaxIntervalMinutes = 60
	#Maximum tax revenue a colony can store before it stops generating further taxes
	#Range: > 1
	MaxTaxRevenue = 5000
	#Enable SDMShop conversion (true = enable, false = disable).
