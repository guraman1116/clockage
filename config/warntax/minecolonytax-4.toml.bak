	#Enable console logging during colony building initialization. Set to false to reduce console spam during server startup. Colony initialization will still occur, just with less verbose logging.
	ShowColonyInitializationLogs = true

["War Settings"]
	#Enable colony ownership transfer when a war is won (true = enable, false = disable).
	EnableColonyTransfer = true
	#If false, war will not toggle any interaction permissions
	EnableWarActions = true
	#If true, war requests must be manually accepted; if false, wars requests will automatically accept.
	WarAcceptanceRequired = true
	#Grace period between declaring wars (minutes)
	#Range: 1 ~ 1440
	AttackerGracePeriodMinutes = 120
	#Grace period between raids (minutes)
	#Range: 1 ~ 1440
	RaidGracePeriodMinutes = 120
	#Maximum raid duration (minutes)
	#Range: 1 ~ 1440
	MaxRaidDurationMinutes = 5
	#Allow players to raid colonies even if the colony owner is offline.
	AllowOfflineRaids = true
	#Penalty percentage applied when a raider is killed by a defender during a raid (0.0 - 1.0)
	#Range: 0.0 ~ 1.0
	RaidPenaltyPercentage = 0.25
	#Percentage of killed raider's balance transferred to defending colony for owner/officers to claim (0.0 - 1.0)
	#Range: 0.0 ~ 1.0
	RaidDefenseRewardPercentage = 0.15
	#Percentage of losing players' balance awarded to each winning player. Set to 0.0 to only enable colony transfer (if enabled).
	#Uses SDMShop balance or colony funds based on what's configured.
	#Range: 0.0 ~ 1.0
	WarVictoryPercentage = 0.25
	#Percentage that each losing player loses from their balance when defeated in war.
	#Uses SDMShop balance or colony funds based on what's configured.
	#Range: 0.0 ~ 1.0
	WarDefeatPercentage = 0.15
	#Percentage that all war participants lose from their balance when a war ends in stalemate.
	#Uses SDMShop balance or colony funds based on what's configured.
	#Range: 0.0 ~ 1.0
	WarStalematePercentage = 0.1
	#Duration (in hours) to freeze colony tax generation after a war loss or stalemate.
	#Set to 0 to disable tax freezing.
	#Range: 0 ~ 168
	WarTaxFreezeHours = 0
	#Minimum number of guards required to initiate a raid. NOTE: This is only used when 'EnableRaidBuildingRequirements' is disabled. If building requirements are enabled, they take priority over this setting.
	#Range: 1 ~ 100
	MinGuardsToRaid = 3
	#Enable raid guard protection system. When enabled, colonies must meet minimum defense requirements to be eligible for raids, protecting smaller/newer colonies from being overwhelmed.
	EnableRaidGuardProtection = true
	#Minimum number of guards a colony must have to be eligible for raids. Set to 0 to disable guard requirement. Default: 2 guards minimum.
	#Range: 0 ~ 100
	MinGuardsToBeRaided = 2
	#Minimum number of guard towers a colony must have to be eligible for raids. Set to 0 to disable guard tower requirement. Default: 1 guard tower minimum.
	#Range: 0 ~ 100
	MinGuardTowersToBeRaided = 1
	#Interval between tax transfers during raids (seconds)
	#Range: 5 ~ 3600
	RaidTaxIntervalSeconds = 60
	#Tax transfer percentages during raids (comma-separated decimals)
	RaidTaxPercentages = [0.1, 0.25, 0.5, 0.7]
	#War duration (minutes)
	#Range: 1 ~ 1440
	WarDurationMinutes = 120
	#Minimum guards required to declare war. NOTE: This is only used when 'EnableWarBuildingRequirements' is disabled. If building requirements are enabled, they take priority over this setting.
	#Range: 1 ~ 100
	MinGuardsToWageWar = 5
	#If enabled, war participants will be switched to the 'war' LP permission group during wars.
	#This requires LuckPerms to be installed and the 'war' group to be properly set up.
	#The command used is: /lp user <Player> parent set war
	EnableLPGroupSwitching = false
	#Duration of the join phase in minutes
	#Range: 1 ~ 30
	JoinPhaseDurationMinutes = 5
	#If enabled, players will keep their inventory on their last life when they die in war.
	#This allows them to continue fighting without losing their gear, and especially important when colony transfer is enabled.
	KeepInventoryOnLastLife = true
	#Number of lives each player has during a war.
	#Range: 1 ~ 100
	PlayerLivesInWar = 5
